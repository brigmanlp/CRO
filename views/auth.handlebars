{{!--HK--}}
<header>
    <h2 class="heading">Training Dashboard</h2>
</header>
<div>
    <div class="jumbotron">
        <h2 class="text-center">Welcome to the Training Area</h2>
        <h3 class="text-center">Click on any of the links below to take you to the training videos.</h3>
        <h3 class="text-center">- CRO</h3>
    </div>

    <div class="container">
        <div class="text-center">
            <div class="panel-heading">
            <h3>Saved Videos</h3>
            </div>
            <div>
                {{#each videos}}
                <div data-id={{id}} class="col-md-3">
                    <img src="/img/ball.png">
                    <a href="{{url}}" target="_blank"><h4>{{title}}</h4></a>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>
